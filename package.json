    "keywords": [
        "waldiez"
    ],
    "license": "Apache-2.0",
    "private": true,
    "homepage": "https://github.com/waldiez/studio",
    "bugs": {
        "url": "https://github.com/waldiez/studio/issues"
    },
    "contributors": [
        {
            "name": "Panagiotis Kasnesis",
            "email": "pkasnesis@thingenious.io"
        },
        {
            "name": "Lazaros Toumanidis",
            "email": "laztoum@protonmail.com"
        },
        {
