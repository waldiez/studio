        }
    ],
    "dependencies": {
        "@waldiez/react": "^0.1.20",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@cspell/eslint-plugin": "^8.16.1",
        "@eslint/compat": "^1.2.4",
        "@eslint/js": "9.16.0",
        "@lcov-viewer/cli": "^1.3.0",
        "@stylistic/eslint-plugin": "^2.12.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.0",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.2",
        "@types/tar-stream": "^3.1.3",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/browser": "^2.1.8",
        "@vitest/coverage-v8": "2.1.8",
        "axios": "^1.7.9",
        "dotenv": "^16.4.7",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react-refresh": "^0.4.16",
        "fs-extra": "^11.2.0",
        "jsdom": "^25.0.1",
        "lcov-result-merger": "^5.0.1",
        "npm-run-all2": "^7.0.1",
        "playwright": "^1.49.1",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "stylelint": "^16.11.0",
        "stylelint-config-recommended": "^14.0.1",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-prettier": "^5.0.2",
        "tar-stream": "^3.1.7",
        "terser": "^5.37.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.0",
        "vite": "^6.0.3",
        "vitest": "^2.1.8"
    },
    "scripts": {
        "dev:front": "yarn monaco && vite -c frontend/vite.config.ts",
        "dev:back": "yarn python -m waldiez_studio --reload --log-level debug",
        "dev": "npm-run-all --parallel dev:front dev:back",
        "check-types": "tsc --noEmit",
        "python": "node --import=tsx frontend/scripts/python.ts",
        "monaco": "node --import=tsx frontend/scripts/monaco.ts",
        "store-last-build-dt": "node --import=tsx frontend/scripts/store-last-build-dt.ts",
        "build:front": "tsc -b && vite -c frontend/vite.config.ts build && yarn store-last-build-dt",
        "build:back": "yarn python scripts/build.py",
        "build": "yarn build:front && yarn build:back",
        "format:front": "yarn prettier && yarn stylelint && yarn eslint",
        "format:back": "yarn python scripts/format.py",
        "format": "yarn format:front && yarn format:back",
        "lint:front": "yarn prettier:check && yarn stylelint:check && yarn eslint:check && yarn check-types",
        "lint:back": "yarn python scripts/lint.py",
        "lint": "yarn lint:front",
        "forlint": "yarn format && yarn lint",
        "eslint": "yarn eslint:check --fix",
        "eslint:check": "eslint --report-unused-disable-directives --max-warnings 0 --cache --config eslint.config.mjs frontend/**/*{.ts,.tsx} --debug",
        "prettier": "prettier \"**/*{.ts,.tsx,.js,.mjs,.css,.json,.md}\" --write --list-different",
        "prettier:check": "prettier \"frontend/**/*{.ts,.tsx,.js,.mjs,.css,.json,.md}\" --check",
        "stylelint": "yarn stylelint:check --fix",
        "stylelint:check": "stylelint --ignore-path .gitignore --cache \"frontend/**/*{.css,.scss}\"",
        "clean:cache": "rimraf .eslintcache .stylelintcache",
        "clean:tsbuild": "rimraf tsconfig.app.tsbuildinfo tsconfig.node.tsbuildinfo tsconfig.tsbuildinfo",
        "clean:dist": "rimraf waldiez_studio/static/frontend",
        "clean:coverage": "rimraf coverage",
        "clean:front": "yarn clean:cache && yarn clean:tsbuild && yarn clean:dist && yarn clean:coverage",
        "clean:back": "yarn python scripts/clean.py",
        "clean": "yarn clean:front && yarn clean:back",
        "before:test": "yarn monaco && yarn playwright install chromium",
        "test:front": "yarn before:test && vitest run --config frontend/vite.config.ts --coverage",
        "test:back": "yarn python -m pytest -c pyproject.toml --cov=waldiez_studio --cov-branch --cov-report term-missing:skip-covered --cov-report html:coverage/backend/html --cov-report xml:coverage/backend/coverage.xml --cov-report lcov:coverage/backend/lcov.info --junitxml=coverage/backend/xunit.xml tests",
        "test": "npm-run-all --parallel test:front test:back && yarn lcov:merge",
        "lcov:merge": "yarn python scripts/merge_lcov.py && yarn node --import=tsx frontend/scripts/lcov_html.ts",
        "lcov:html": "lcov-viewer lcov -o coverage/html coverage/lcov.info",
        "requirements:back": "yarn python -m pip install -r requirements/all.txt",
        "requirements:front": "yarn install",
        "requirements": "yarn requirements:front && yarn requirements:back",
        "all:front": "yarn requirements:front && yarn clean:front && yarn format:front && yarn lint:front && yarn test:front && yarn build:front",
        "all:back": "yarn requirements:back && yarn clean:back && yarn format:back && yarn lint:back && yarn test:back && yarn build:back",
        "all": "yarn requirements && yarn clean && yarn format && yarn lint && yarn test && yarn build"
    },
    "packageManager": "yarn@4.5.3"
}
